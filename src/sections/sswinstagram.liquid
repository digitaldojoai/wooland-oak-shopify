<div class="ssw-instagram-feed-wrap container">
    <div class="section clearfix ac">

      <a href="https://www.instagram.com/woolandoak/" target="_blank"><img class='insta-header' style="max-width: 330px;" src="{{ 'instagram.png' | asset_url }})" /></a>
      <div class="sixteen columns">
        <div class="clearfix placeholder-instagram">
          <div class="ssw-html-widget ssw-instagram-widget" data-module="service" data-name="instagramPhotos" data-gallery_id="{{section.settings.option_galleries_id}}" data-type="{{section.settings.option_font}}">
            <div class="ssw-page-loading">
              <span class="ssw-spin ssw-loader"></span>
            </div>
          </div>
        </div>
      </div>
    </div>
</div>


